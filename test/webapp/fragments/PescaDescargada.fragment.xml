<core:FragmentDefinition controllerName="com.tasa.test.controller.PescaDescargada" xmlns="sap.ui.table" xmlns:m="sap.m" xmlns:u="sap.ui.unified" xmlns:f="sap.f" xmlns:core="sap.ui.core">

	<m:Panel id="0009" expandable="false">
		<m:content>
			<Table
				id="table_pesc_desc_ticket"
				selectionMode="MultiToggle"
				rows="{/ProductCollection}"
				visibleRowCount="3"
				enableCellFilter="{ui>/cellFilterOn}"
				ariaLabelledBy="title">
				<extension>
					<m:OverflowToolbar id="ext_pesc_desc" style="Clear" >
						<m:Title id="title2" text="Cantidad de pesca en declarada: "/>
						<m:Text text="50 "/>
						<m:Text text="TM"/>
						<m:ToolbarSpacer/>
						
					</m:OverflowToolbar>
				</extension>
				<columns>
					<Column
						id="pdt_col_BuscarDesc"
						width="4rem">
						<m:Label text="" />
						<template>
							<m:Text text="{Name}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="pdt_col_EliminarDesc"
						width="4rem">
						<m:Label text="" />
						<template>
							<m:Text text="{Name}" wrapping="false" />
						</template>
					</Column>
					<Column
						width="11rem">
						<m:Label text="Nro. descarga" />
						<template>
							<m:Text text="{Name}" wrapping="false" />
						</template>
					</Column>
					<Column
						width="11rem"
						filterProperty="Category"
						defaultFilterOperator="StartsWith">
						<m:Label text="Ticket" />
						<template>
							<m:Text text="{Category}" wrapping="false" />
						</template>
					</Column>
					<Column width="9rem">
						<m:Label text="Especie" />
						<template>
							<m:Link text="Show Image" href="{ProductPicUrl}" target="_blank"/>
						</template>
					</Column>
					<Column width="9rem">
						<m:Label text="Desc. especie" />
						<template>
							<m:Link text="Show Image" href="{ProductPicUrl}" target="_blank"/>
						</template>
					</Column>
					<Column
						id="availability2"
						width="9rem"
						filterProperty="Available"
						showFilterMenuEntry="false"
						defaultFilterOperator="EQ"
						filterType="sap.ui.model.type.Boolean">
						<m:Label text="Cant. descargada" />
						<template>
							<m:ObjectStatus text="{Status}" state="{
								path: 'Available',
								formatter: '.formatAvailableToObjectState'
							}"/>
						</template>
					</Column>
					<Column
						id="price2"
						width="9rem"
						filterProperty="Price">
						<m:Label text="Cant. declarada" />
						<template>
							<u:Currency value="{Price}" currency="{CurrencyCode}"/>
						</template>
					</Column>
					<Column
						width="10rem"
						hAlign="End"
						filterProperty="Quantity"
						filterType="sap.ui.model.type.Integer">
						<m:Label text="Pto. Descarga" />
						<template>
							<m:Label text="{
								path: 'Quantity',
								type: 'sap.ui.model.type.Integer'
							}" />
						</template>
					</Column>
					<Column
						width="10rem"
						hAlign="End"
						filterProperty="Quantity"
						filterType="sap.ui.model.type.Integer">
						<m:Label text="Lado descarga" />
						<template>
							<m:Label text="{
								path: 'Quantity',
								type: 'sap.ui.model.type.Integer'
							}" />
						</template>
					</Column>
					<Column
						width="10rem"
						hAlign="End"
						filterProperty="Quantity"
						filterType="sap.ui.model.type.Integer">
						<m:Label text="Fecha/hora inicio" />
						<template>
							<m:Label text="{
								path: 'Quantity',
								type: 'sap.ui.model.type.Integer'
							}" />
						</template>
					</Column>
					<Column
						width="10rem"
						hAlign="End"
						filterProperty="Quantity"
						filterType="sap.ui.model.type.Integer">
						<m:Label text="Fecha/hora fin" />
						<template>
							<m:Label text="{
								path: 'Quantity',
								type: 'sap.ui.model.type.Integer'
							}" />
						</template>
					</Column>
					<Column
						width="10rem"
						hAlign="End"
						filterProperty="Quantity"
						filterType="sap.ui.model.type.Integer">
						<m:Label text="Fecha de producción" />
						<template>
							<m:Label text="{
								path: 'Quantity',
								type: 'sap.ui.model.type.Integer'
							}" />
						</template>
					</Column>
				</columns>
				
			</Table>
			<Table
				id="table_pesc_desc_especie"
				selectionMode="MultiToggle"
				rows="{/ProductCollection}"
				visibleRowCount="3"
				enableCellFilter="{ui>/cellFilterOn}"
				ariaLabelledBy="title">
				<columns>
					<Column
						id="pde_col_EliminarDesc"
						width="4rem">
						<m:Label text="" />
						<template>
							<m:Text text="{Name}" wrapping="false" />
						</template>
					</Column>
					<Column
						width="4rem">
						<m:Label text="" />
						<template>
							<m:Text text="{Name}" wrapping="false" />
						</template>
					</Column>
					<Column
						width="11rem">
						<m:Label text="Nro. descarga" />
						<template>
							<m:Text text="{Name}" wrapping="false" />
						</template>
					</Column>
					<Column width="9rem">
						<m:Label text="Especie" />
						<template>
							<m:Link text="Show Image" href="{ProductPicUrl}" target="_blank"/>
						</template>
					</Column>
					<Column width="9rem">
						<m:Label text="Desc. especie" />
						<template>
							<m:Link text="Show Image" href="{ProductPicUrl}" target="_blank"/>
						</template>
					</Column>
					<Column
						id="availability4"
						width="9rem"
						filterProperty="Available"
						showFilterMenuEntry="false"
						defaultFilterOperator="EQ"
						filterType="sap.ui.model.type.Boolean">
						<m:Label text="Cant. descargada" />
						<template>
							<m:ObjectStatus text="{Status}" state="{
								path: 'Available',
								formatter: '.formatAvailableToObjectState'
							}"/>
						</template>
					</Column>
					<Column
						id="price4"
						width="9rem"
						filterProperty="Price">
						<m:Label text="Cant. declarada" />
						<template>
							<u:Currency value="{Price}" currency="{CurrencyCode}"/>
						</template>
					</Column>
				</columns>
				
			</Table>
			<Table
				id="table_pesc_desc_CHD"
				selectionMode="MultiToggle"
				rows="{/ProductCollection}"
				visibleRowCount="3"
				enableCellFilter="{ui>/cellFilterOn}"
				ariaLabelledBy="title">
				<columns>
					<Column
						id="pdCHD_col_BuscarDesc"
						width="4rem">
						<m:Label text="" />
						<template>
							<m:Text text="{Name}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="pdCHD_col_EliminarDesc"
						width="4rem">
						<m:Label text="" />
						<template>
							<m:Text text="{Name}" wrapping="false" />
						</template>
					</Column>
					<Column
						width="11rem">
						<m:Label text="Nro. descarga" />
						<template>
							<m:Text text="{Name}" wrapping="false" />
						</template>
					</Column>
					<Column
						width="11rem"
						filterProperty="Category"
						defaultFilterOperator="StartsWith">
						<m:Label text="Ticket" />
						<template>
							<m:Text text="{Category}" wrapping="false" />
						</template>
					</Column>
					<Column width="9rem">
						<m:Label text="Especie" />
						<template>
							<m:Link text="Show Image" href="{ProductPicUrl}" target="_blank"/>
						</template>
					</Column>
					<Column width="9rem">
						<m:Label text="Desc. especie" />
						<template>
							<m:Link text="Show Image" href="{ProductPicUrl}" target="_blank"/>
						</template>
					</Column>
					<Column
						id="availability3"
						width="9rem"
						filterProperty="Available"
						showFilterMenuEntry="false"
						defaultFilterOperator="EQ"
						filterType="sap.ui.model.type.Boolean">
						<m:Label text="Cant. descargada" />
						<template>
							<m:ObjectStatus text="{Status}" state="{
								path: 'Available',
								formatter: '.formatAvailableToObjectState'
							}"/>
						</template>
					</Column>
					<Column
						id="price3"
						width="9rem"
						filterProperty="Price">
						<m:Label text="Cant. pesca" />
						<template>
							<u:Currency value="{Price}" currency="{CurrencyCode}"/>
						</template>
					</Column>
					<Column
						width="10rem"
						hAlign="End"
						filterProperty="Quantity"
						filterType="sap.ui.model.type.Integer">
						<m:Label text="Cant. declarada" />
						<template>
							<m:Label text="{
								path: 'Quantity',
								type: 'sap.ui.model.type.Integer'
							}" />
						</template>
					</Column>
					<Column
						width="10rem"
						hAlign="End"
						filterProperty="Quantity"
						filterType="sap.ui.model.type.Integer">
						<m:Label text="Lado descarga" />
						<template>
							<m:Label text="{
								path: 'Quantity',
								type: 'sap.ui.model.type.Integer'
							}" />
						</template>
					</Column>
					<Column
						width="10rem"
						hAlign="End"
						filterProperty="Quantity"
						filterType="sap.ui.model.type.Integer">
						<m:Label text="Fecha de producción" />
						<template>
							<m:Label text="{
								path: 'Quantity',
								type: 'sap.ui.model.type.Integer'
							}" />
						</template>
					</Column>
				</columns>
				
			</Table>

		</m:content>
	</m:Panel>

</core:FragmentDefinition>